[module/wired-up]
type = internal/network
interface = eno1
interval = 2
format-connected-prefix = " "
format-connected-suffix = "  "
format-connected-prefix-foreground = ${root.ethernet}
format-connected-prefix-background = ${root.ethernet-bg}
format-connected-suffix-background = ${root.ethernet-bg}
label-connected = %upspeed:8%
label-connected-foreground = ${root.ethernet}
label-connected-background = ${root.ethernet-bg}
label-disconnected = 

[module/wired-down]
type = internal/network
interface = enp7s0
interval = 2
format-connected-prefix = " "
format-connected-suffix = "  "
format-connected-prefix-foreground = ${root.ethernet-alt}
format-connected-prefix-background = ${root.ethernet-bg}
format-connected-suffix-background = ${root.ethernet-bg}
label-connected = %downspeed:8%
label-connected-foreground = ${root.ethernet-alt}
label-connected-background = ${root.ethernet-bg}
label-disconnected = 

[module/wired-condensed]
type = internal/network
interface = enp7s0
interval = 2
format-connected-prefix-foreground = ${root.wifi-alt}
format-connected-prefix-background = ${root.wifi-bg}
format-connected-suffix-background = ${root.wifi-bg}
label-connected = "%upspeed:8%   %downspeed:8%"
label-connected-foreground = ${root.wifi-alt}
label-connected-background = ${root.wifi-bg}